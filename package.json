{
  "name": "data-table-component",
  "version": "1.0.10",
  "description": "The DataTable component is a reusable React component that provides functionality for displaying tabular data in a paginated and sortable manner. It allows users to specify the number of entries displayed per page, apply filters to the data, and sort the data based on column headers.",
  "license": "MIT",
  "author": "ClÃ©ment STORNE",
  "keywords": [
    "datatable",
    "data table",
    "data filter",
    "table pagination"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/data-table-component.umd.js",
  "module": "./dist/data-table-component.es.js",
  "exports": {
    ".": {
      "import": "./dist/data-table-component.es.js",
      "require": "./dist/data-table-component.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "c8": "^8.0.1",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
